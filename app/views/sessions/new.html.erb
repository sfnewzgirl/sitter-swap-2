<div class="container-fluid">
  <div class="row welcome-header clearfix">
    <div class="col-lg-6 col-sm-12" id="sitter-swap-logo">
      <h1>SitterSwap</h1>
    </div>
    <div class="col-lg-6 col-sm-12" id="welcome-header-links">
      <%= link_to 'About', about_path %>
      <%= link_to 'Join', new_person_path %>
      <%= link_to 'Log In', login_path %>
      <%= link_to 'Home', root_path %>
    </div>
  </div>
</div>
<div class="container-fluid form-container">
  <div class="row">
    <div class="col-lg-12">
      <h1>Welcome back! Log in here:</h1>
        <%= form_for @person, url: '/sessions' do |f| %>
        <div class="log-in-header"><h1>Email</h1></div>
        <%= f.email_field :email, placeholder: 'Email'%>
        <div class="log-in-header"><h1>Password</h1></div>
        <%= f.password_field :password, placeholder: 'Password' %>
        <%= f.submit 'Submit', class: 'btn btn-default btn-submit'%>
        <% end %>
    </div>
  </div>
</div>
