<div class="container-fluid">
  <div class="row welcome-header clearfix">
    <div class="col-lg-6 col-sm-12" id="sitter-swap-logo">
      <h1>SitterSwap</h1>
    </div>
    <div class="col-lg-6 col-sm-12" id="welcome-header-links">
      <%= link_to 'About', about_path %>
      <%= link_to 'Join', new_person_path %>
      <%= link_to 'Log In', login_path %>
    </div>
  </div>
</div>
<div class="container-fluid" id="log-in-container">
  <h1>Log In</h1>
    <div class="row">
      <div class="col-sm-12">
        <%= form_for @person, url: '/sessions' do |f| %>
        <p>Email</p>
        <%= f.email_field :email, placeholder: 'Email'%>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <p>Password</p>
        <%= f.password_field :password, placeholder: 'Password' %>
      </div>
    </div>
        <%= f.submit 'Submit', class: 'btn btn-default'%>
      </div>
    </div>
  <% end %>
</div>
